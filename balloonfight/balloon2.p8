debug={}

function debug:draw()
	local status=''
	for message in all(self.messages) do
		status=status..' '..message
	end
	--print(status,0,0,7)
end

function debug:input()
	local key=stat(31)
	
	if key=='b' then
		player.balloons+=1
		if player.balloons>2 then
			player.balloons=1
		end
	end
	
	if key=='d' then
		player.balloons=0
	end
	
	if key=='l' then
		player.lives+=1
		if player.lives>2 then
			player.lives=0
		end
	end
	
	if key=='f' then
		balloons:new()
	end
	
	if key=='s' then
		player.score+=1
	end
	
end

function debug:update()
	self.messages={
		'grnd:'..(player.grounded and '1' or '0'),
		'vely:'..player.velocity.y,
		'velx:'..player.velocity.x
	}
	self:input()
end
