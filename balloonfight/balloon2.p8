version 32
__lua__
-- balloon fight
-- andrew stephens
-- july 2021
-- version 0.1

debug=true
debug_messages={}
test_mode=true
ticks=0
left=0
right=1

-- physics/speeds
gravity=0.1
force=gravity*2
bounce=force*2
aspeed=0.2
aspeed_max=3
gspeed=0.2
gspeed_max=2

-- change game mode
function change_mode(mode)
	mode:init()
end

-- check for collisions
function collision(rect1,rect2)
	return rect1.x < rect2.x + rect2.width and
   rect1.x + rect1.width > rect2.x and
   rect1.y < rect2.y + rect2.height and
   rect1.y + rect1.height > rect2.y
end

-- lerp
function lerp(a, b, t)
	return a+(b-a)*t
end

-- get map loc based on x,y
maploc=function(x,y)
 local col=flr((x+7)/8)
 local row=flr((y+7)/8)
 return {
 	col=col,
 	row=row
 }
end

-- pad with zeros
function pad(value,length)
	local string=tostr(value)
 if (#string==length) return string
 return "0"..pad(string, length-1)
end

-------------------------------
-- bonus mode
-------------------------------

bonus_mode={}

function bonus_mode:init()
	bgstars:init()
	level:get()
	_update=self.update
	_draw=self.draw
end

function bonus_mode:draw()
 cls(1)
 bgstars:draw()
 level:draw()
 pipes:draw()
 balloons:draw()
 osd:draw()
 player:draw()
	debug:draw()
end

function bonus_mode:update()
	ticks=ticks+1
 bgstars:update()
 balloons:update()
 pipes:update()
 player:update()
 debug:update()
end

-------------------------------
-- play mode
-------------------------------

play_mode={}

function play_mode:init()
 bgstars:init()
 level:get()
	_update=self.update
	_draw=self.draw
end

function play_mode:update()
	ticks=ticks+1
 bgstars:update()
 balloons:update()
 player:update()
 debug:update()
end

function play_mode:draw()
 cls(1)
 bgstars:draw()
 balloons:draw()
 level:draw()
 osd:draw()
 player:draw()
	debug:draw()
end

-------------------------------
-- pico-8 callbacks
-------------------------------

function _init()
	palt(0,false)
	palt(1,true)
	change_mode(bonus_mode)
end

