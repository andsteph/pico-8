-- pipes

pipes={
	units={},
	counter=0,
	timer=0,
	delay=10,
}

function pipes:draw()
	for unit in all(self.units) do
		spr(132,unit.x,unit.y)
	end
end

function pipes:new(col)
	local pipe={
		x=(col-1)*8,
		y=112
	}
	add(self.units,pipe)
end

function pipes:update()
	self.timer+=1
	if self.timer>self.delay then
		self.timer=0
		balloons:new()
	end
end

